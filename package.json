{
  "name": "inventaire-client",
  "description": "a libre collaborative resources mapper powered by open-knowledge (client-side)",
  "license": "AGPL-3.0",
  "homepage": "https://inventaire.io",
  "repository": {
    "type": "git",
    "url": "http://github.com/inventaire/inventaire-client"
  },
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "imports": {
    "#app/*": "./app/*.js",
    "#assets/*": "./app/assets/*.js",
    "#behaviors/*": "./app/modules/general/behaviors/*.js",
    "#components/*": "./app/modules/general/components/*.js",
    "#entities/*": "./app/modules/entities/*.js",
    "#general/*": "./app/modules/general/*.js",
    "#groups/*": "./app/modules/groups/*.js",
    "#inventory/*": "./app/modules/inventory/*.js",
    "#lib/*": "./app/lib/*.js",
    "#listings/*": "./app/modules/listings/*.js",
    "#map/*": "./app/modules/map/*.js",
    "#modules/*": "./app/modules/*.js",
    "#network/*": "./app/modules/network/*.js",
    "#notifications/*": "./app/modules/notifications/*.js",
    "#search/*": "./app/modules/search/*.js",
    "#settings/*": "./app/modules/settings/*.js",
    "#shelves/*": "./app/modules/shelves/*.js",
    "#tasks/*": "./app/modules/tasks/*.js",
    "#tests/*": "./tests/*.js",
    "#transactions/*": "./app/modules/transactions/*.js",
    "#user/*": "./app/modules/user/*.js",
    "#users/*": "./app/modules/users/*.js",
    "#vendor/*": "./vendor/*.js",
    "#welcome/*": "./app/modules/welcome/*.js"
  },
  "scripts": {
    "build": "./scripts/build",
    "build:dev": "webpack --config ./bundle/webpack.config.dev.cjs",
    "generate-sitemaps": "./scripts/sitemaps/generate.js",
    "lint": "./scripts/lint",
    "lint-fix": "./scripts/lint_fix",
    "lint-fix-staged": "./scripts/lint_staged fix",
    "lint-staged": "./scripts/lint_staged",
    "postinstall": "./scripts/postinstall",
    "start": "webpack serve --config ./bundle/webpack.config.dev.cjs",
    "test": "./scripts/run_unit_tests",
    "update-i18n": "./scripts/build_i18n",
    "update-jsconfig": "./scripts/generate_jsconfig.cjs > ./jsconfig.json",
    "update-toc": "doctoc README.md",
    "watch": "npm start",
    "watch-without-hot-reload": "webpack watch --config ./bundle/webpack.config.dev.cjs"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "backbone": "^1.3.3",
    "backbone-filtered-collection": "^0.4.0",
    "backbone-nested": "^2.0.4",
    "backbone.marionette": "^4.1.3",
    "backbone.radio": "^2.0.0",
    "blueimp-canvas-to-blob": "^3.28.0",
    "cropper": "^2.3.0",
    "cropperjs": "^1.5.12",
    "enumerate-devices": "^1.1.0",
    "fork-awesome": "^1.2.0",
    "handlebars": "^4.7.6",
    "isbn3": "^1.1.6",
    "jquery": "^3.6.0",
    "jquery-inview": "^1.1.2",
    "jquery-visible": "^1.2.0",
    "js-cookie": "^2.2.1",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "leven": "^2.1.0",
    "marionette.approuter": "^1.0.2",
    "node-polyglot": "git+https://github.com/inventaire/polyglot.js.git#9804c41",
    "normalize.css": "^8.0.1",
    "p-timeout": "^4.1.0",
    "papaparse": "^4.6.3",
    "quagga": "^0.12.1",
    "svelte": "^4.2.0",
    "svelte-bricks": "^0.2.0",
    "underscore": "^1.8.3",
    "wikidata-lang": "^2.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/runtime": "^7.18.3",
    "@types/underscore": "^1.11.4",
    "babel-loader": "^8.2.5",
    "circular-dependency-plugin": "^5.2.2",
    "config": "^3.3.3",
    "core-js": "^3.23.2",
    "css-loader": "^5.0.1",
    "cssnano": "^5.0.11",
    "doctoc": "^1.4.0",
    "eslint": "^8.48.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte": "^2.33.0",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.5.3",
    "iconv-lite": "^0.4.24",
    "mini-css-extract-plugin": "^2.4.5",
    "mocha": "^9.2.0",
    "node-fetch": "^2.6.1",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.1",
    "sass": "^1.44.0",
    "sass-loader": "^12.6.0",
    "should": "^13.2.3",
    "style-loader": "^2.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-use-logical": "^2.1.0",
    "svelte-loader": "^3.1.9",
    "svelte-preprocess-sass": "^2.0.1",
    "svelte-reactive-preprocessor": "^0.8.3",
    "tiny-chalk": "^2.0.0",
    "unused-webpack-plugin": "^2.4.0",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "wikidata-sdk": "^5.16.2"
  },
  "browserslist": [
    "> 0.5%",
    "Firefox ESR",
    "last 3 years",
    "not op_mini all",
    "not dead"
  ]
}
