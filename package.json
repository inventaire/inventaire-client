{
  "name": "inventaire-client",
  "description": "a libre collaborative resources mapper powered by open-knowledge (client-side)",
  "license": "AGPL-3.0",
  "homepage": "https://inventaire.io",
  "repository": {
    "type": "git",
    "url": "http://github.com/inventaire/inventaire-client"
  },
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "imports": {
    "#app/*": "./app/*.js",
    "#assets/*": "./app/assets/*.js",
    "#behaviors/*": "./app/modules/general/behaviors/*.js",
    "#components/*": "./app/modules/general/components/*.js",
    "#entities/*": "./app/modules/entities/*.js",
    "#general/*": "./app/modules/general/*.js",
    "#inventory/*": "./app/modules/inventory/*.js",
    "#lib/*": "./app/lib/*.js",
    "#map/*": "./app/modules/map/*.js",
    "#modules/*": "./app/modules/*.js",
    "#network/*": "./app/modules/network/*.js",
    "#notifications/*": "./app/modules/notifications/*.js",
    "#search/*": "./app/modules/search/*.js",
    "#settings/*": "./app/modules/settings/*.js",
    "#shelves/*": "./app/modules/shelves/*.js",
    "#tasks/*": "./app/modules/tasks/*.js",
    "#transactions/*": "./app/modules/transactions/*.js",
    "#user/*": "./app/modules/user/*.js",
    "#users/*": "./app/modules/users/*.js",
    "#vendor/*": "./vendor/*.js",
    "#welcome/*": "./app/modules/welcome/*.js"
  },
  "scripts": {
    "build": "./scripts/build",
    "build:dev": "webpack --config ./bundle/webpack.config.dev.cjs",
    "edit-mentions": "libreoffice ./scripts/assets/mentions.csv && ./scripts/update_mentions.js",
    "generate-sitemaps": "./scripts/sitemaps/generate.js",
    "lint": "./scripts/lint",
    "lint-fix": "./scripts/lint_fix",
    "lint-staged": "./scripts/lint_staged",
    "postinstall": "./scripts/postinstall",
    "start": "webpack serve --config ./bundle/webpack.config.dev.cjs",
    "test": "./scripts/run_unit_tests",
    "update-i18n": "./scripts/update_i18n",
    "update-jsconfig": "./scripts/generate_jsconfig.cjs > ./jsconfig.json",
    "update-mentions": "./scripts/update_mentions.js",
    "update-toc": "doctoc README.md",
    "watch": "npm start"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "backbone": "^1.3.3",
    "backbone-filtered-collection": "^0.4.0",
    "backbone-nested": "^2.0.4",
    "backbone.marionette": "^4.1.3",
    "blueimp-canvas-to-blob": "^3.28.0",
    "cropper": "^2.3.0",
    "cropperjs": "^1.5.12",
    "enumerate-devices": "^1.1.0",
    "fork-awesome": "^1.2.0",
    "handlebars": "^4.7.6",
    "imagesloaded": "^4.1.4",
    "isbn3": "^1.1.6",
    "jquery": "^3.6.0",
    "jquery-inview": "^1.1.2",
    "jquery-visible": "^1.2.0",
    "js-cookie": "^2.2.1",
    "leaflet": "^1.7.1",
    "leaflet.markercluster": "^1.4.1",
    "leven": "^2.1.0",
    "marionette.approuter": "^1.0.2",
    "masonry-layout": "^4.2.2",
    "node-polyglot": "git+https://github.com/inventaire/polyglot.js.git#9804c41",
    "normalize.css": "^8.0.1",
    "p-timeout": "^4.1.0",
    "papaparse": "^4.6.3",
    "quagga": "^0.12.1",
    "underscore": "^1.8.3",
    "wikidata-lang": "^2.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/runtime": "^7.16.3",
    "babel-loader": "^8.2.3",
    "circular-dependency-plugin": "^5.2.2",
    "config": "^3.3.3",
    "css-loader": "^5.0.1",
    "cssnano": "^5.0.11",
    "doctoc": "^1.4.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-svelte3": "^3.2.1",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.5.0",
    "iconv-lite": "^0.4.24",
    "mini-css-extract-plugin": "^2.4.5",
    "mocha": "^9.2.0",
    "node-fetch": "^2.6.1",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.44.0",
    "sass-loader": "^12.6.0",
    "should": "^13.2.3",
    "sinon": "^1.17.3",
    "style-loader": "^2.0.0",
    "svelte": "^3.46.4",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess-sass": "^2.0.1",
    "tiny-chalk": "^2.0.0",
    "unused-webpack-plugin": "^2.4.0",
    "webpack": "^5.69.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "wikidata-sdk": "^5.16.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "browserslist": [
    "> 0.25%",
    "last 2 versions",
    "Firefox ESR",
    "last 5 years",
    "not dead"
  ]
}
